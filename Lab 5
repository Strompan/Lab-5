def cast(to_type):
    def decorator(func):
        def wrapper(*args, **kwargs):
            result = func(*args, **kwargs)
            try:
                return to_type(result)
            except (ValueError, TypeError):
                return result
        return wrapper
    return decorator

@cast(float)
def divide(a, b):
    return a / b

result = divide(10, 3)

print(result)   # Выведет: 3.3333333333333335
print(type(result))   # Выведет: <class 'float'>
